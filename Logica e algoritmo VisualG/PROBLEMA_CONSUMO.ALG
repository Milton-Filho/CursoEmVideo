Algoritmo "problema_consumo"

Var

    distancia : real
    combustivel : real
    consumo : real



Inicio


      escreva("Digite a distância percorrida: ")
      leia(distancia)
      escreva("Informe o combustivel gasto: ")
      leia(combustivel)

      consumo <- distancia / combustivel
      
      escreva("Consumo médio = ",consumo:8:3)
      

      

      
      


Fimalgoritmo