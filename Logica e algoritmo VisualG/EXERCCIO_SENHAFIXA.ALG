Algoritmo "exerccio_SenhaFixa"

Var

   senha : inteiro


Inicio

      escreva("Digite a senha: ")
      leia(senha)
      
      enquanto senha <> "3lm@m@gr3ll@" faca
               escreva("Senha Invalida! Tente novamente: ")
               leia(senha)
      fimenquanto
      
      escreva("Acesso permitido!")


Fimalgoritmo